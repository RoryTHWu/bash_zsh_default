alias commit='git add . && git commit -m'
alias push='git push'
alias pull='git pull'
alias status='git status'
alias mkdir='mkdir -pv'
alias ls='ls -F --color=auto'
alias rm='rm -I'
alias mv='mv -i'
alias cp='cp -i'
alias ln='ln -i'
alias psa='ps aux' # a for all users, u for processes belonging to the current $
alias bmi='ssh oddjobs.bmi.emory.edu'
